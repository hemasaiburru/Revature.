<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Login Page - Revature Shop</title>
    <style>
        body {
            font-family: Arial, sans-serif;
                        background-image: url('data:image/jpeg;base64,'); /* Replace with your image URL */
            background-size: cover; /* Cover the entire screen */
            background-position: center; /* Center the image */
            background-repeat: no-repeat; /* Prevent repetition */
            text-align: center;
            margin: 0;
            padding: 0;
        }
        h1 {
            font-size: 50px;
            color: rgb(69, 141, 10);
            margin-top: 20px;
        }
        .container {
            display: flex;
            justify-content: space-around;
            margin-top: 50px;
            z-index: 1;
            position: relative;
        }
        .column {
            background-color: rgba(255, 255, 255, 0.8);
            padding: 20px;
            width: 30%;
            border: 1px solid #ccc;
            border-radius: 10px;
            box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.1);
        }
        .column h2 {
            font-size: 28px;
            color: black;
            margin-bottom: 20px;
        }
        input[type="text"], input[type="password"] {
            width: 100%;
            padding: 10px;
            margin: 15px 0;
            border: 1px solid #ccc;
            border-radius: 4px;
        }
        input[type="submit"] {
            width: 100%;
            padding: 10px;
            background-color: #28a745;
            color: white;
            border: none;
            cursor: pointer;
            border-radius: 4px;
        }
        input[type="submit"]:hover {
            background-color: #218838;
        }
        .register-link {
            display: block;
            text-align: center;
            margin-top: 20px;
            color: black;
            text-decoration: underline;
        }
    </style>
</head>
<body>
    <h1>Welcome to RevShop</h1>
    
    <div class="container">
        <div class="column">
            <h2>Buyer Login</h2>
            <div class="img-container">
                <img src="data:image/jpeg;base64," alt="Buyer Image">
            </div>
            <form action="/ecommProject/LoginServlet" method="post">
                <input type="hidden" name="userType" value="buyer">
                <input type="text" name="username" placeholder="Enter Username" required>
                <input type="password" name="password" placeholder="Enter Password" required>
                <input type="submit" value="Login as Buyer">
            </form>
            <a href="<%= request.getContextPath() %>/ServerController?userType=buyer_register" class="register-link">Don't have an account? Register here</a>
        </div>

        <div class="column">
            <h2>Seller Login</h2>
            <div class="img-container">
                <img src="data:image/jpeg;base64," alt="Seller Image">
            </div>
            
            <form action="/ecommProject/LoginServlet" method="post">
                <input type="hidden" name="userType" value="seller">
                <input type="text" name="username" placeholder="Enter Username" value="bbhemasai@gmail.com"required>
                <input type="password" name="password" placeholder="Enter Password" value="Hemasai@1234"required>
                <input type="submit" value="Login as Seller">
            </form>
            <a href="<%= request.getContextPath() %>/ServerController?userType=seller_register" class="register-link">Don't have an account? Register here</a>
        </div>
    </div>

    <script>
        <% 
            String errorMessage = (String) request.getAttribute("errorMessage");
            if (errorMessage != null) { 
        %>
            alert("<%= errorMessage %>");
        <% 
            } 
        %>
    </script>
    <%@ include file="Footer.jsp" %>
</body>
</html>
